{
    "name": "akali22/sendcloud",
    "description": "Provides a client to interact with the Sendcloud API in an object-oriented way.",
    "keywords": [
        "sendcloud",
        "client",
        "api",
        "jouwweb",
        "parcel",
        "webhook"
    ],
    "homepage": "https://github.com/akali22/sendcloud",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "akali22",
            "email": "akali22@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "JouwWeb\\Sendcloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\JouwWeb\\Sendcloud\\": "test"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1.0"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0",
        "symfony/var-dumper": "^6.0.0"
    }
}
