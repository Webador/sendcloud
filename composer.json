{
    "name": "retrobak/sendcloud",
    "description": "Provides a client to interact with the Sendcloud API in an object-oriented way.",
    "keywords": [
        "sendcloud",
        "client",
        "api",
        "jouwweb",
        "parcel",
        "webhook"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sjoerd Nuijten",
            "email": "sjoerd@jouwweb.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "JouwWeb\\SendCloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\JouwWeb\\SendCloud\\": "test"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.5"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3.0|^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.7",
        "symfony/var-dumper": "^4.2"
    }
}
